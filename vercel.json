{
  "version": 2,
  "functions": {
    "api/index.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/items", "methods": ["GET", "POST"], "dest": "api/index.js" },
    { "src": "/items/(?<id>\\d+)", "methods": ["PUT", "DELETE"], "dest": "api/index.js" }
  ]
}
